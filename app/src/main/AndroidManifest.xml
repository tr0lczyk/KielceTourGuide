<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.kielcetourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/herb_kielc"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/herb_kielc"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- Parent activity meta-data to support 4.0 and lower -->
        <activity android:name=".ItemDescription"
            android:label="@string/doing"
            android:parentActivityName="com.example.android.kielcetourguide.MainActivity"/>
    </application>

</manifest>