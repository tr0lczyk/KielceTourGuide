<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.kielcetourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/herb_kielc"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/herb_kielc"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SeeActivity"
            android:label="@string/what_to_see"/>
        <activity android:name=".EatActivity"
            android:label="@string/where_to_eat" />
        <activity android:name=".SleepActivity"
            android:label="@string/where_to_sleep" />
        <activity android:name=".DoingActivity"
            android:label="@string/doing"></activity>
    </application>

</manifest>